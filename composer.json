{
    "name": "ngyuki/route-collector",
    "description": "Simple route collector",
    "type": "library",
    "license": "MIT",
    "keywords": [ "router" ],
    "homepage": "https://github.com/ngyuki/php-route-collector",
    "authors": [
        {
            "name": "Toshiyuki Goto",
            "email": "ngyuki.jp@gmail.com"
        }
    ],
    "config": {
        "platform": {
            "php": "7.0.10"
        }
    },
    "autoload": {
        "psr-4": {
            "ngyuki\\RouteCollector\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ngyuki\\RouteCollector\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.0.10"
    },
    "require-dev": {
        "nikic/fast-route": "^1.2",
        "phpunit/phpunit": "^6.3",
        "satooshi/php-coveralls": "^1.0 | ^2.0"
    },
    "suggest": {
        "nikic/fast-route": "FastRoute Integration"
    }
}
